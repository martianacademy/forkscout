{
    "$schema": "./packages/agent/src/config.schema.json",
    "provider": "openrouter",
    "temperature": 1,
    "fallbackProviders": [],
    "agent": {
        "port": 3210,
        "owner": "Suru",
        "maxIterations": 100,
        "maxSteps": 80,
        "effortStepsQuick": 15,
        "effortStepsModerate": 25,
        "forkscoutMemoryMcpUrl": "http://127.0.0.1:3211/mcp",
        "autoRegisterTools": true,
        "dynamicToolLoading": true,
        "failureEscalationThreshold": 3,
        "activityLogMaxBytes": 5242880,
        "browserIdleMs": 60000,
        "mcpServers": {
            "context7": {
                "command": "npx",
                "args": [
                    "-y",
                    "@upstash/context7-mcp"
                ]
            },
            "deepwiki": {
                "url": "https://mcp.deepwiki.com/mcp"
            },
            "sequential-thinking": {
                "command": "npx",
                "args": [
                    "-y",
                    "@modelcontextprotocol/server-sequential-thinking"
                ]
            },
            "github": {
                "command": "npx",
                "args": [
                    "-y",
                    "@modelcontextprotocol/server-github"
                ],
                "env": {
                    "GITHUB_PERSONAL_ACCESS_TOKEN": "$GITHUB_API_KEY"
                }
            },
            "playwright": {
                "command": "npx",
                "args": [
                    "-y",
                    "@playwright/mcp"
                ]
            }
        },
        "server": {
            "rateLimitLocal": 300,
            "rateLimitRemote": 30,
            "rateLimitWindowMs": 60000,
            "maxBodyBytes": 1048576
        },
        "subAgent": {
            "tier": "balanced",
            "timeoutMs": 600000,
            "maxSteps": 20,
            "maxParallel": 5,
            "outputMaxLength": 5000,
            "retryAttempts": 5,
            "retryDelayMs": 1000,
            "batchTimeoutMs": 0,
            "temperature": 0.5
        },
        "telegram": {
            "maxInbox": 200,
            "maxHistory": 20
        }
    },
    "router": {
        "openrouter": {
            "fast": {
                "model": "qwen/qwen3-coder-next"
            },
            "balanced": {
                "model": "qwen/qwen3-coder-next"
            },
            "powerful": {
                "model": "qwen/qwen3-coder-next"
            }
        },
        "google": {
            "fast": {
                "model": "gemini-2.5-flash"
            },
            "balanced": {
                "model": "gemini-3-flash-preview"
            },
            "powerful": {
                "model": "gemini-3-pro-preview"
            }
        },
        "anthropic": {
            "fast": {
                "model": "claude-haiku-4.5"
            },
            "balanced": {
                "model": "claude-sonnet-4.6"
            },
            "powerful": {
                "model": "claude-opus-4.6"
            }
        },
        "openai": {
            "fast": {
                "model": "gpt-5-mini"
            },
            "balanced": {
                "model": "gpt-5"
            },
            "powerful": {
                "model": "gpt-5.2-codex"
            }
        },
        "openai-compatible": {
            "fast": {
                "model": "claude-sonnet-4-5-20250929"
            },
            "balanced": {
                "model": "claude-sonnet-4-5-20250929"
            },
            "powerful": {
                "model": "claude-sonnet-4-5-20250929"
            }
        },
        "github": {
            "fast": {
                "model": "claude-sonnet-4.6"
            },
            "balanced": {
                "model": "claude-sonnet-4.6"
            },
            "powerful": {
                "model": "claude-sonnet-4.6"
            }
        },
        "copilot-bridge": {
            "fast": {
                "model": "claude-sonnet-4.6"
            },
            "balanced": {
                "model": "claude-sonnet-4.6"
            },
            "powerful": {
                "model": "claude-sonnet-4.6"
            }
        }
    },
    "searxng": {
        "url": "http://localhost:8888"
    }
}