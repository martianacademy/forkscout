{
    "telegram": {
        "pollingTimeout": 30,
        "historyTokenBudget": 12000,
        "ownerUserIds": [],
        "allowedUserIds": [],
        "rateLimitPerMinute": 20,
        "maxInputLength": 2000,
        "ownerOnlyTools": [
            "run_shell_commands",
            "write_file"
