{
    "name": "@forkscout/agent",
    "version": "0.1.0",
    "description": "Forkscout agent engine with RAG and memory",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "dev": "tsx watch --exclude '.forkscout/**' --exclude '**/*.bak' src/cli.ts",
        "start": "tsx src/cli.ts",
        "serve": "tsx src/serve.ts",
        "serve:dist": "tsx dist/serve.js",
        "serve:dist:watch": "node scripts/watch-dist.mjs",
        "watchdog": "./watchdog.sh",
        "watchdog:dev": "./watchdog.sh --dev",
        "build": "tsc",
        "clean": "rm -rf dist tsconfig.tsbuildinfo",
        "lint": "eslint src --ext .ts",
        "lint:fix": "eslint src --ext .ts --fix",
        "format": "prettier --write 'src/**/*.ts'",
        "format:check": "prettier --check 'src/**/*.ts'",
        "test": "vitest"
    },
    "dependencies": {
        "@ai-sdk/anthropic": "^3.0.44",
        "@ai-sdk/google": "^3.0.29",
        "@ai-sdk/openai": "^3.0.29",
        "@modelcontextprotocol/sdk": "^1.26.0",
        "ai": "^6.0.86",
        "dotenv": "^17.3.1",
        "gpt-tokenizer": "^3.4.0",
        "playwright": "^1.49.1",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/node": "^22.0.0",
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@typescript-eslint/parser": "^8.0.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^10.0.0",
        "prettier": "^3.4.0",
        "tsx": "^4.19.2",
        "typescript": "^5.7.2",
        "vitest": "^2.1.8"
    },
    "directories": {
        "example": "examples"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "type": "commonjs"
}